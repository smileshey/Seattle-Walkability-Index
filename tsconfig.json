{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "baseUrl": "./src", // Add this line if not present
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": [
    "src"
  ]
}


